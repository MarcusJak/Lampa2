<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presets">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:text="Lampan kommer att sättas på"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lampan kommer att stängas av"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.354" />

    <EditText
        android:id="@+id/onhour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:hint="timme"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />



    <EditText
        android:id="@+id/ofhour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:hint="timme"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/offmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="minut"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/ofhour"
        app:layout_constraintStart_toStartOf="@+id/ofhour"
        app:layout_constraintTop_toBottomOf="@+id/ofhour" />

    <EditText
        android:id="@+id/onmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="minut"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="@+id/onhour"
        app:layout_constraintTop_toBottomOf="@+id/onhour" />

    <Button
        android:id="@+id/alarm1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lägg till alarm"
        app:layout_constraintEnd_toEndOf="@+id/onmin"
        app:layout_constraintStart_toStartOf="@+id/onmin"
        app:layout_constraintTop_toBottomOf="@+id/onmin" />

    <Button
        android:id="@+id/Alarm2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lägg till alarm"
        app:layout_constraintEnd_toEndOf="@+id/offmin"
        app:layout_constraintStart_toStartOf="@+id/offmin"
        app:layout_constraintTop_toBottomOf="@+id/offmin" />
</androidx.constraintlayout.widget.ConstraintLayout>